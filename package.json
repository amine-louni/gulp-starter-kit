{
  "name": "gulp-starter-kit",
  "version": "2.0.0",
  "private": true,
  "description": "Gulp Starter Kit for front-end developers.",
  "bugs": {
    "url": "https://github.com/nstungcom/gulp-starter-kit/issues",
    "email": "hi@nstung.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nstungcom/gulp-starter-kit.git"
  },
  "license": "MIT",
  "author": "Son Tung Ngo <hi@nstung.com>",
  "main": "gulpfile.babel.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm-run-all clean --parallel eleventy:watch gulp:dev || exit 0",
    "build": "npm-run-all clean --parallel eleventy:build gulp:build",
    "eleventy:watch": "eleventy --quiet --watch || exit 0",
    "eleventy:build": "eleventy || exit 0",
    "gulp:dev": "gulp --silent || exit 0",
    "gulp:build": "cross-env NODE_ENV=production gulp build || exit 0"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "autoprefixer": "^9.6.4",
    "babelify": "^10.0.0",
    "browser-sync": "^2.26.7",
    "browserify": "^16.5.0",
    "core-js": "^3.2.1",
    "critical": "^1.3.6",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-gzip": "^1.4.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^6.1.1",
    "gulp-postcss": "^8.0.0",
    "gulp-purgecss": "^1.2.0",
    "gulp-rev-all": "^2.0.2",
    "gulp-rev-delete-original": "^0.2.3",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^9.0.0",
    "gulp-uglify": "^3.0.2",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "js-beautify": "^1.10.2",
    "js-yaml": "^3.13.1",
    "markdown-it": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.11.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
